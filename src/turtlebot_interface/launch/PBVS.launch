  <!-- PBVS Controller -->
  <include file="$(find aruco)/launch/controller_demo.launch"/>

  <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" if="$(arg debug)"/>

  <node name="rqt_image_view" pkg="rqt_image_view" type="rqt_image_view" if="$(arg debug)"
	args="/aruco_marker_publisher/result"/>
</launch>
